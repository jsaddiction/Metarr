    "@tanstack/react-query": "^5.90.2",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^2.0.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
